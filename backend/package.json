{
  "name": "harmonyforge-backend",
  "version": "1.0.0",
  "description": "HarmonyForge backend API server for music harmonization",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "build:full": "npm run build:frontend && npm run build",
    "build:frontend": "cd ../frontend && npm run build",
    "start": "node dist/server.js",
    "start:unified": "npm run build:full && npm start",
    "clean": "rm -rf dist uploads/*"
  },
  "keywords": [
    "music",
    "harmonization",
    "api",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "xmldom": "^0.6.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/xmldom": "^0.1.34",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
