{
  "name": "harmonyforge",
  "version": "1.0.0",
  "description": "HarmonyForge - Music Harmonization Application (Frontend Only)",
  "type": "module",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install": "cd frontend && npm install",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "verify-instruments": "node scripts/verify-instruments.cjs",
    "clean": "rm -rf frontend/dist frontend/node_modules backend/node_modules node_modules"
  },
  "keywords": [
    "music",
    "harmonization",
    "frontend"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.x"
  }
}
